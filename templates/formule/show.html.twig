{% extends 'admin/layout.html.twig' %}
{% block title %} {{parent()}} | Formule {% endblock %}

{% block dataStructured %}{% endblock %}
{% block body %}
    <h1>Formule</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ formule.id }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ formule.message }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ formule.price }}</td>
            </tr>
            <tr>
                <th>Month</th>
                <td>{{ formule.month }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ formule.name }}</td>
            </tr>
            <tr>
                <th>Price_shipping</th>
                <td>{{ formule.priceShipping }}</td>
            </tr>
            <tr>
                <th>Try_days</th>
                <td>{{ formule.tryDays }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('formule_index') }}">back to list</a>

    <a href="{{ path('formule_edit', {'id': formule.id}) }}">edit</a>

    {{ include('formule/_delete_form.html.twig') }}
{% endblock %}
